{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "PostgreSQL",
    "EnableAutoMigration": false,
    "EnableSensitiveDataLogging": false,
    "CommandTimeout": 30,
    "MaxRetryCount": 3
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=propostadb;Username=postgres;Password=postgres"
  },
  "AWS": {
    "Profile": "default",
    "Region": "us-east-1",
    "SNS": {
      "PropostaAprovadaTopic": "arn:aws:sns:us-east-1:000000000000:proposta-aprovada"
    }
  }
}

